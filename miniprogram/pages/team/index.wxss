view, scroll-view, block, input, button, text, image, video, canvas {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-size: inherit;
  color: inherit;
  font-style: inherit;
  line-height: inherit; }

input {
  width: 100%;
  display: block;
  height: 70rpx;
  border: 2rpx solid #ddd;
  border-radius: 6rpx;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #555; }

.button {
  height: 70rpx;
  display: block;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 0;
  background: #fa8c35;
  font-size: 30rpx;
  color: #fff;
  border-radius: 6rpx; }

.team {
  width: 100%;
  min-height: 100vh;
  background: #f6f6f6;
  padding: 20rpx; }
  .team > .li {
    background: #fff;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    height: 200rpx; }
    .team > .li > image {
      width: 200rpx;
      height: 200rpx;
      margin-right: 20rpx; }
    .team > .li > .div {
      flex: 1; }
      .team > .li > .div > .h3 {
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
        color: #333;
        font-size: 30rpx;
        line-height: 1.5;
        margin-bottom: 10rpx; }
      .team > .li > .div > .h4 {
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
        color: #aaa;
        font-size: 20rpx; }
      .team > .li > .div > .h5 {
        margin-top: 20rpx;
        color: #555;
        font-size: 26rpx;
        line-height: 1.5;
        height: 80rpx;
        overflow: hidden; }
  .team .button {
    margin-top: 100rpx;
    width: 700rpx;
    background: #fa8c35;
    color: #fff;
    font-size: 30rpx;
    border-radius: 10rpx;
    height: 70rpx; }
    .MyNavigator{
      position: fixed;
      z-index: 10;
      width: 100%;
      height: 140rpx;
      background-color: #fa8c35;
      text-align: center;
      line-height: 180rpx;
      font-size: 34rpx;
      font-weight: 550;
      border: solid 2rpx #a9a9ad;
    }
